<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section attributeName="browserSettings" type="AutomatedTestingFramework.Core.Config.BrowserSettingsConfiguration, AutomatedTestingFramework.Core" />
	</configSections>
	<browserSettings driverLocation="Drivers\" defaultBrowser="Chrome" scriptTimeout="10" pageLoadTimeout="10" implicitWaitTimeout="8" />
	<appSettings>
		<add key="BaseUrl" value="http://localhost/" />
		<add key="DefaultTestUserattributeName" value="userattributeName" />
		<add key="DefaultTestPassword" value="password" />
		<add key="MediaFolderPath" value="C:\AutomatedTestVideos\UnitTests" />
		<add key="AllowVideoRecording" value="true" />
		<add key="VideoRecordingFrameRate" value="10" />
		<add key="VideoRecordingQuality" value="20" />
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
		</assemblyBinding>
	</runtime>
</configuration>
